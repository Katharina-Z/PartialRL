#response latency for omisssion not included
#if collection latency 0, interpret as NaN
#exclude collection latencies for premature, omissions and incorrect

#Two tau values for response latency and collection latency
TauResp = data.RespLat
TauColl = data.CollLat


data.RewSum = data.CR

data.Tau = data.RespLat + data.CollLat



for i in data.RewSum
    data.RewSum[i] = data.RewSum[i] + data.RewSum[i-1]
    println(i)
end


for i=1:size(data.RewSum)
    data.RewSum = data.RewSum[i] + data.RewSum[i-1]
end


